@model IEnumerable<Lab_Assignment2_WhistPointCalculator.Games>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
          

            <th>
                @Html.DisplayNameFor(model => model.Ended)
            </th>
            
           
            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
           
            <td>
                @Html.DisplayFor(modelItem => item.Ended)
            </td>
          
            <td>|
                <a asp-action="Details" asp-route-id="@item.GamesId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.GamesId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
